{
  "name": "df youtube angular",
  "description": "This extension is made for demonstration purposes",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage", 
    "activeTab",
    "scripting",
    "tabs",
    "webNavigation"
  ],
  "host_permissions": ["https://www.youtube.com/*"],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}